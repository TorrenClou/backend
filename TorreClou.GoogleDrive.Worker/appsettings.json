{
  "ConnectionStrings": {
    "DefaultConnection": "Host=localhost;Database=torrenclo;Username=torrenclo_user;Password=your_password_here"
  },
  "Redis": {
    "ConnectionString": "localhost:6379"
  },
  "JobLease": {
    "LeaseDurationMinutes": 5,
    "LeaseRefreshIntervalSeconds": 30
  },
  "GoogleDrive": {
    "ClientId": "your_google_drive_client_id.apps.googleusercontent.com",
    "ClientSecret": "your_google_drive_client_secret",
    "Scopes": [
      "https://www.googleapis.com/auth/drive.file",
      "https://www.googleapis.com/auth/userinfo.email"
    ]
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  }
}
